.navbar.navbar-default.navbar-static-top{:role => "navigation"}
  .container
    .navbar-header
      /%a{id:'logo-circle' href:'.'} MM
      =link_to "MM", root_path, id: 'logo-circle'
      %button.navbar-toggle{ data: { toggle:"collapse", target:"#collapsed-navbar" } } 
        %span.sr-only
          Toggle Navigation
        %i.fa.fa-plus.fw
      /= link_to "Memeify", root_path, id: "navbar-brand"
    .collapse.navbar-collapse#collapsed-navbar
      %ul.nav.navbar-nav.navbar-right
        = render 'layouts/navigation_links'